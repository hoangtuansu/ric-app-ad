{
        "name": "ad-tuan",
        "version": "1.0.2",
        "containers": [
            {
                "name": "ad",
                "image": {
                    "registry": "registry.hub.docker.com",
                    "name": "hoangtuansu/ad",
                    "tag": "0.1"
                }
            },
            {
                "name": "predict",
                "image": {
                    "registry": "registry.hub.docker.com",
                    "name": "hoangtuansu/ad",
                    "tag": "0.1"
                },
                "command": ["sh", "-c", "python insert.py"]
            }
        ],
        "appenv": {
            "INFLUXDB_PASSWORD": "CemexAw3adl4jv0EKEiYO26Smjx8TmOOQEpxQd5Wcy51EX7Q1O8qrcic1JGCL8mEG2jS0-xExW4T-6TtkYldCw=="
        },
        "messaging": {
            "ports": [
                {
                    "name": "http",
                    "container": "ad",
                    "port": 8080,
                    "description": "http service"
                },
                {
                    "name": "rmr-data",
                    "container": "ad",
                    "port": 4560,
                    "txMessages": ["TS_ANOMALY_UPDATE"],
                    "rxMessages": ["TS_ANOMALY_ACK"],
                    "policies": [],
                    "description": "rmr receive data port for ad"
                },
                {
                    "name": "rmr-route",
                    "container": "ad",
                    "port": 4561,
                    "description": "rmr route port for ad"
                }
            ]
        },
        "rmr": {
            "protPort": "tcp:4560",
            "maxSize": 2072,
            "numWorkers": 1,
            "rxMessages": ["TS_ANOMALY_ACK"],
            "txMessages": ["TS_ANOMALY_UPDATE"],
            "policies": []
        }

}

